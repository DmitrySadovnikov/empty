<table border="1">
  <thead>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>status</th>
    <th>created_at</th>
  </tr>
  </thead>
  <tbody>
  <% torrent_files.each do |torrent_file| %>
    <tr>
      <td><%= torrent_file.id %></td>
      <td><%= torrent_file.name %></td>
      <td><%= torrent_file.status %></td>
      <td><%= torrent_file.created_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>
